{
    "themeDirectory" : "./rei/wp-content/themes/astra/",
    "customizerFiles": [
        "inc/customizer/custom-controls/responsive/",
        "inc/customizer/custom-controls/divider/",
        "inc/customizer/custom-controls/heading/",
        "inc/customizer/custom-controls/description/",
        "inc/customizer/custom-controls/radio-image/",
        "inc/customizer/custom-controls/slider/",
        "inc/customizer/custom-controls/sortable/",
        "inc/customizer/custom-controls/spacing/",
        "inc/customizer/custom-controls/responsive-spacing/"
    ],
    "editorFiles" : {
        "src": "sass/editor-style.scss",
        "dest": "assets/css/unminified/"
    },
    "sass" : {
        "root" : "sass/",
        "site" : "sass/site/",
        "compatibility" : "sass/site/compatibility/",
        "woocommerce"   : "sass/site/compatibility/woocommerce/"
    },
    "assets": {
        "root": "assets/",
        "cssroot" : "assets/css/",
        "css"   : {
            "unminified" : {
                "root": "assets/css/unminified/",
                "compatibility": "assets/css/unminified/compatibility",
                "woocommerce" : "assets/css/unminified/compatibility/woocommerce"
            },
            "minified" : "assets/css/minified/"
        }
    }
}